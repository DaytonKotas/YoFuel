{
  "name": "yofuel",
  "version": "1.0.0",
  "scripts": {
    "start:mongo": "mongod --dbpath C:/data/db --port 27017",
    "start:backend": "cd backend && node server.js",
    "start:frontend": "cd frontend && npm start",
    "start:all": "concurrently \"npm run start:mongo\" \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^6.2.1"
  }
}
